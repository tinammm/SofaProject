<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textColorPrimary">@color/surface1_light</item>
    </style>

    <style name="CustomTabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/surface1</item>
    </style>

    <style name="standingsTextHeadline">
        <item name="android:textColor">?attr/n_lv_2</item>
        <item name="android:textSize">12sp</item>
        <item name="android:spacing">-0.28sp</item>
    </style>

    <style name="standingsText">
        <item name="android:textColor">?attr/n_lv_1</item>
        <item name="android:textSize">12sp</item>
        <item name="android:spacing">-0.28sp</item>
    </style>

    <style name="assistiveText">
        <item name="android:textColor">?attr/n_lv_1</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">end</item>
        <item name="android:ems">10</item>
        <item name="android:maxLines">2</item>
    </style>

    <style name="microText">
        <item name="android:textColor">?attr/n_lv_1</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="headlineOneText">
        <item name="android:textColor">?attr/n_lv_2</item>
        <item name="android:textSize">32sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="headlineOneDarkText">
        <item name="android:textColor">?attr/n_lv_1</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="body">
        <item name="android:textColor">?attr/n_lv_1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:ems">10</item>
        <item name="android:maxLines">2</item>
    </style>

    <style name="headlineThree">
        <item name="android:textColor">?attr/n_lv_1</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="tournamentLogoShape">
        <item name="cornerSize">8dp</item>
    </style>
</resources>